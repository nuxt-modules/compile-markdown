{
  "name": "nuxt-compile-markdown",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.15.3",
  "description": "Nuxt Module to compile markdown files into Vue SFC at build time.",
  "license": "MIT",
  "repository": "nuxt-modules/compile-markdown",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "bumpp -x \"npx changelogen --output=CHANGELOG.log\" && pnpm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.3",
    "@shikijs/markdown-it": "^1.1.6",
    "markdown-it": "^14.0.0",
    "markdown-it-mdc": "^0.2.3",
    "shiki": "^1.1.6",
    "unplugin-vue-markdown": "^0.26.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.3",
    "@nuxt/test-utils": "3.7.0",
    "@types/node": "^20.11.20",
    "bumpp": "^9.3.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "nuxt": "^3.10.3",
    "vitest": "^1.3.1"
  }
}
